{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    
    "extends": [
        "xo/esnext",
        "xo-typescript",
        "xo-typescript/space"
    ],
    "rules": {
        "indent": "off"
    },
      
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "plugins": [
        "@typescript-eslint"
    ]
}
